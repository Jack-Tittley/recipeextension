{
    "manifest_version": 3,
    "name": "Recipe Bookmark Extension",
    "version": "1.0",
    "description": "Bookmark the current URL and view saved list",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "tabs",
      "scripting",
      "storage",
      "activeTab"
    ],
   
    
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}